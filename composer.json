{
    "type": "spec-pattern",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/orm": "^2.15",
        "doctrine/dbal": "^3.6",
        "doctrine/annotations": "^2.0",
        "vlucas/phpdotenv": "^5.5",
        "symfony/cache": "^6.2",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1"
    }
}
