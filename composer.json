{
    "type": "spec-pattern",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/orm": "^2.11",
        "doctrine/dbal": "^3.3",
        "doctrine/annotations": "^1.13",
        "vlucas/phpdotenv": "^5.4",
        "symfony/cache": "^6.0",
        "ramsey/uuid": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5",
        "jetbrains/phpstorm-attributes": "^1.0"
    }
}
